<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_search_results"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.tom_roush.musclesloth.SearchResultsActivity">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Free Weights"
        android:ems="10"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:background="@null"
        android:padding="10px"
        android:enabled="false"
        android:layout_toEndOf="@+id/imageView2"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:id="@+id/editText"
        android:textSize="22sp"
        android:textColor="@color/colorPrimary" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text='Search Results for "bench":'
        android:ems="10"
        android:id="@+id/editText3"
        android:enabled="false"
        android:textSize="25sp"
        android:padding="10px"
        android:layout_alignParentLeft="true"
        android:background="@null"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="@color/colorPrimary" />

    <ImageView
        android:layout_width="50dp"
        app:srcCompat="@drawable/dumbbells"
        android:id="@+id/imageView2"
        android:layout_height="50dp"
        android:layout_below="@+id/editText3"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:id="@+id/freeListView"
        android:layout_below="@+id/editText"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        app:srcCompat="@drawable/gears"
        android:layout_below="@+id/freeListView"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2"
        android:layout_marginTop="11dp"
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:enabled="false"
        android:background="@null"
        android:padding="10px"
        android:text="Machines"
        android:ems="10"
        android:id="@+id/editText4"
        android:textSize="22sp"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="?attr/colorPrimary" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:enabled="false"
        android:text="none found"
        android:ems="10"
        android:id="@+id/editText8"
        android:background="@null"
        android:textSize="12sp"
        android:padding="15px"

        android:textStyle="normal|italic"
        android:textColor="?attr/colorControlNormal"
        android:layout_below="@+id/editText7"
        android:layout_alignLeft="@+id/imageView8"
        android:layout_alignStart="@+id/imageView8" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Cardio"
        android:ems="10"
        android:background="@null"
        android:id="@+id/editText7"
        android:enabled="false"
        android:padding="10px"
        android:textSize="22sp"
        android:textColor="?attr/colorPrimary"
        android:layout_alignBottom="@+id/imageView8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/cardio"
        android:id="@+id/imageView8"
        android:layout_below="@+id/editText6"
        android:layout_alignLeft="@+id/editText6"
        android:layout_alignStart="@+id/editText6"
        android:layout_marginTop="12dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="none found"
        android:enabled="false"
        android:background="@null"
        android:padding="15px"
        android:ems="10"
        android:id="@+id/editText6"
        android:textSize="12sp"
        android:textStyle="normal|italic"
        android:textColor="?attr/colorControlNormal"
        android:layout_below="@+id/imageView4"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4" />

</RelativeLayout>

